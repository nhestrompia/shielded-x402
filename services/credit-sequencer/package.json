{
  "name": "@shielded-x402/credit-sequencer",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "pretest": "pnpm --filter @shielded-x402/shared-types build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "lint": "tsc -p tsconfig.json --noEmit",
    "dev": "tsx src/server.ts",
    "migrate:up": "tsx src/db/migrate.ts"
  },
  "dependencies": {
    "@shielded-x402/shared-types": "workspace:*",
    "express": "^5.1.0",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.16.3",
    "viem": "^2.39.3",
    "zod": "^4.3.6"
  }
}
